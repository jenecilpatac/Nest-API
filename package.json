    "cache-manager": "^6.3.1",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.1",
    "cookie-parser": "^1.4.7",
